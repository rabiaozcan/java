
public class DikdortgenKapsulleme {
	private double en, boy;
	public double getEn() {
		return en;
	}
	public void setEn(double en) {
		this.en = en;
	}
	public double getBoy() {
		return boy;
	}
	public void setBoy(double boy) {
		this.boy = boy;
	}
	public double DikdortgenAlani1() {
		return en*boy;
	}
}
